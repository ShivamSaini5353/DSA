public static void main(String[] args) {
       int num=4;
       int a=0, b=1;
       for(int i=0; i<num; i++) {
    	   if(i==0) System.out.print(a+" ");
    	   else if(i==1) System.out.print(b+" ");
    	   else {
    	   int c=a+b;
    	   a=b;
    	   b=c;
    	   System.out.print(c+" ");
    	   }
       }
