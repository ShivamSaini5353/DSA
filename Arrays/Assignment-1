//Question - 1
	public static int[] targetSum(int nums[], int target) {
		int i=0, j=nums.length-1;
		int ans[]=new int[2];
		while(i<=j) {
			if(nums[i]+nums[j]==target) {
				ans[0]=i;
				ans[1]=j;
				break;
			}else if(nums[i]+nums[j]>target)j--;
			else i++;
		}
		return ans;
	}

//Question-2








